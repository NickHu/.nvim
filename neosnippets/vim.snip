# ------------------------------------------------------------------
# dein.vim
snippet     dein#add
abbr        dein#add('...')
	call dein#add('${1}')${0}

snippet     dein#add#on_ft
abbr        dein#add('...', {'on_ft': ['...']})
	call dein#add('${1}', {'on_ft': ['${2}']})${0}

snippet     dein#on_ft
abbr        , {'on_ft': ['...'}
	, {'on_ft': ['${1}']}

snippet     dein#on_ft#cfamily
abbr        , {'on_ft': ['c', 'cpp', 'objc', 'objcpp']}
	, {'on_ft': ['c', 'cpp', 'objc', 'objcpp']}
